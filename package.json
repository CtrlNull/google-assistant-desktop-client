{
  "name": "ga-desktop",
  "version": "0.1.0",
  "author": "Wendell Misiedjan",
  "description": "A Google Assistant desktop Client",
  "license": "n/a",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:noasar": "node .electron-vue/build.js && electron-builder --dir --asar=false",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint ./src/renderer/ --ext js,vue --max-warnings 0",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "Google Assistant for Desktop",
    "appId": "org.digispark.ga-desktop",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "artyom.js": "^1.0.6",
    "axios": "^0.16.2",
    "co": "^4.6.0",
    "dotenv": "^5.0.0",
    "eslint-plugin-pug": "^1.1.1",
    "fs": "0.0.1-security",
    "google-assistant-node": "github:WMisiedjan/google-assistant-node#feature/v1alpha2",
    "googleapis": "^25.0.0",
    "grpc": "^1.8.4",
    "metaget": "^1.0.6",
    "mkdirp": "^0.5.1",
    "msr": "^1.3.4",
    "node-fetch": "^1.7.3",
    "opn": "^5.1.0",
    "path": "^0.12.7",
    "pcm-convert": "^1.6.5",
    "perfect-scrollbar": "^1.3.0",
    "pump": "^2.0.1",
    "pumpify": "^1.4.0",
    "robotjs": "^0.4.7",
    "spotify-web-api-node": "^2.5.0",
    "spotify-web-helper": "^1.12.0",
    "v-bar": "^3.0.0",
    "vue": "^2.5.13",
    "vue-electron": "^1.0.6",
    "vue-perfect-scrollbar": "^0.1.0",
    "vue-router": "^3.0.1",
    "vuetify": "0.17.6",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "bootstrap-sass": "^3.3.7",
    "cfonts": "^1.1.3",
    "chai": "^4.1.2",
    "chalk": "^2.3.0",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "1.7.11",
    "electron-builder": "^19.56.2",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.3",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-vue": "^4.2.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.1",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.2.2",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.9",
    "mocha": "^4.1.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "require-dir": "^0.3.2",
    "spectron": "^3.8.0",
    "style-loader": "^0.19.1",
    "stylelint": "^8.2.0",
    "stylelint-config-concentric": "^2.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^2.3.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.6.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.7.1",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1"
  }
}
