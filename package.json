{
	"name": "ga-desktop",
	"version": "0.4.0",
	"author": "Wendell Misiedjan",
	"description": "A Google Assistant desktop Client",
	"license": "n/a",
	"main": "./dist/electron/main.js",
	"scripts": {
		"build": "node .electron-vue/build.js && electron-builder",
		"build:dir": "node .electron-vue/build.js && electron-builder --dir",
		"build:noasar": "node .electron-vue/build.js && electron-builder --dir --asar=false",
		"build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
		"dev": "node .electron-vue/dev-runner.js",
		"e2e": "npm run pack && mocha test/e2e",
		"lint": "eslint ./src/renderer/ --ext js,vue --max-warnings 0",
		"lint:fix": "eslint ./src/renderer/ --ext js,vue --fix",
		"pack": "npm run pack:main && npm run pack:renderer",
		"pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
		"pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
		"test": "npm run unit && npm run e2e",
		"unit": "karma start test/unit/karma.conf.js",
		"postinstall": "electron-builder install-app-deps",
		"start": "npm run dev"
	},
	"dependencies": {
		"artyom.js": "^1.0.6",
		"axios": "^0.18.0",
		"co": "^4.6.0",
		"dotenv": "^6.1.0",
		"eslint-plugin-pug": "^1.1.1",
		"fs": "0.0.1-security",
		"google-assistant": "github:WMisiedjan/google-assistant#master",
		"googleapis": "^25.0.0",
		"grpc": "^1.11.3",
		"jquery": "^3.3.1",
		"metaget": "^1.0.6",
		"mkdirp": "^0.5.1",
		"msr": "^1.3.4",
		"node-fetch": "^1.7.3",
		"opn": "^5.3.0",
		"path": "^0.12.7",
		"pcm-convert": "^1.6.5",
		"perfect-scrollbar": "^1.3.0",
		"pump": "^2.0.1",
		"pumpify": "^1.5.1",
		"robotjs": "^0.5.1",
		"spotify-web-api-node": "^3.1.1",
		"spotify-web-helper": "^1.13.0",
		"v-bar": "^3.0.0",
		"vue": "^2.5.16",
